.panel.panel-default
  .panel-heading
    %h3.panel-title= t(:summary)
  .panel-body
    %table.table.table-normal
      %tr
        %td.bold= t(:total_incomes)
        %td= @entries[:total_incomes]
      %tr
        %td.bold= t(:total_expenses)
        %td= @entries[:total_expenses]
      %tr
        %td.bold= t(:spare_amount)
        %td= @entries[:balance]
